
public class Counter {
	int count = 0;
	
	public int Increament()
	{
		this.count++;
		return this.count;
	}
	public int Decreament() {
		this.count--;
		return this.count;
	}
	public int getCounts()
	{
		return this.count;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
